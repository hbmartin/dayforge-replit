  "jscpd": {
    "threshold": 0,
    "reporters": [
      "markdown",
      "console"
    ],
    "ignore": [
      "**/node_modules/**",
      "**/dist/**",
      "**/coverage/**",
      "**/tests/**",
      "**/.next/**",
      "**/*.json",
