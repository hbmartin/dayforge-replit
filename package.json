{
  "name": "dayforge",
  "version": "0.0.0",
  "private": false,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "b": "NEXT_TELEMETRY_DISABLED=1 next build --debug",
    "start": "next start",
    "lint": "biome check --error-on-warnings",
    "lint:report": "biome check . --reporter=json | jq -r '  .diagnostics[]  | .category' | sort | uniq -c | sort -nr",
    "format": "biome check --write",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "pnpm exec tsx modules/kernel/infrastructure/db/migrate.ts",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit pull",
    "db:check": "drizzle-kit check",
    "db:up": "drizzle-kit up",
    "admin:bootstrap": "tsx scripts/bootstrap-admin.ts",
    "test": "vitest run --silent=passed-only",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:playwright": "export PLAYWRIGHT=True && pnpm exec playwright test",
    "check": "pnpm format && pnpm dupes && pnpm test && pnpm build",
    "knip": "knip",
    "dupes": "jscpd"
  },
